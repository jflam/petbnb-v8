{
  "name": "ai-starter-app-postgis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:client": "vite",
    "dev:server": "nodemon src/server/simplified-server.js",
    "build": "npm run build:client",
    "build:client": "vite build",
    "start": "node src/server/simplified-server.js",
    "seed": "node scripts/seed.js",
    "generate-images": "node scripts/generate-images.js",
    "seed:full": "npm run generate-images && npm run seed",
    "migrate": "node-pg-migrate up -m migrations",
    "test:db:create": "node scripts/manage-test-db.js create",
    "test:db:drop": "node scripts/manage-test-db.js drop",
    "test:db:setup": "npm run test:db:create && DATABASE_URL=postgresql://postgres:postgres@localhost:5432/petbnb_test npm run migrate && DATABASE_URL=postgresql://postgres:postgres@localhost:5432/petbnb_test npm run seed",
    "test:db:reset": "node scripts/manage-test-db.js reset && DATABASE_URL=postgresql://postgres:postgres@localhost:5432/petbnb_test npm run migrate && DATABASE_URL=postgresql://postgres:postgres@localhost:5432/petbnb_test npm run seed",
    "setup-test-db": "bash ./scripts/setup-test-db.sh",
    "test": "vitest run --coverage",
    "test:client": "vitest run tests/client",
    "test:server": "vitest run tests/server",
    "test:e2e": "playwright test",
    "test:all": "npm run test && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@types/mapbox-gl": "^3.4.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.3.1",
    "express": "^5.0.0-rc.1",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "mapbox-gl": "^3.12.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.15.0",
    "pg-cursor": "^2.10.3",
    "pino": "^8.16.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.6.0",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "execa": "^9.5.3",
    "jsdom": "^24.0.0",
    "node-pg-migrate": "^7.9.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "pg-connection-string": "^2.6.2",
    "pino-pretty": "^10.2.3",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^6.0.0",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
